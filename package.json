        "@types/node": "^18.6.4",
        "@typescript-eslint/eslint-plugin": "^5.42.0",
        "@typescript-eslint/parser": "^5.42.0",
        "eslint": "^8.26.0",
        "prettier": "^2.7.1",
        "rimraf": "^4.0.4",
        "rollup": "^3.2.5",
        "typescript": "^4.7.4",
